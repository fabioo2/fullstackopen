onst [neutral, setNeutral] = useState(0);\n    const [bad, setBad] = useState(0);\n\n    const handleGoodClick = () => {\n        setGood(good + 1);\n    };\n    const handleNeutralClick = () => {\n        setNeutral(neutral + 1);\n    };\n    const handleBadClick = () => {\n        setBad(bad + 1);\n    };\n\n    return (\n        <div className=\"row justify-content-center align-items-center\">\n            <div className=\"m-5 d-flex flex-column\">\n                <h2 className=\"pt-2\">Give Feedback</h2>\n                <div className=\"align-self-center\">\n                    <ButtonGroup>\n                        <FeedbackButton\n                            onClick={handleGoodClick}\n                            text=\"good\"\n                            color=\"success\"\n                        />\n                        <FeedbackButton\n                            onClick={handleNeutralClick}\n                            text=\"neutral\"\n                            color=\"secondary\"\n                        />\n                        <FeedbackButton\n                            onClick={handleBadClick}\n                            text=\"bad\"\n                            color=\"danger\"\n                        />\n                    </ButtonGroup>\n                </div>\n                <h2 className=\"pt-2\">Statistics</h2>\n                <Statistics good={good} bad={bad} neutral={neutral} />\n            </div>\n        </div>\n    );\n};\n\nReactDOM.render(<App />, document.getElementById('root'));\n",{"ruleId":"9","severity":1,"message":"10","line":7,"column":5,"nodeType":"11","messageId":"12","endLine":7,"endColumn":14},{"ruleId":"9","severity":1,"message":"13","line":9,"column":5,"nodeType":"11","messageId":"12","endLine":9,"endColumn":14},"no-unused-vars","'Container' is defined but never used.","Identifier","unusedVar","'Jumbotron' is defined but never used."]